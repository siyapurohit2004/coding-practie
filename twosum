# include<iostream>
using namespace std;
int main(){
 int num ,target,arr;
int arr={2,7,5,11};
int target =9;
 for(int i=0;i<4;i++){
    for(int j=i+1;j<4;j++){
        if (num[i]+num[j]==target){
            arr[0]=i;
            arr[1]=j;
            break;
        }
    }
 }
 return arr;
}